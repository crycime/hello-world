{
  "extends": "prettier",
  "env":{
    "node":true,
    "es6":true
  },
  "ecmaFeatures": {
    "jsx": true,
    "impliedStrict":true
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": "off",
    "no-console":"off",
    "no-unused-vars":"off",
    "no-unreachable":"off",
    "no-redeclare":"warn"
  }
}